{
  "name": "fieldwork",
  "description": "feasibility study",
  "version": "0.9.10",
  "author": "Shin Ogata",
  "bugs": {
    "url": "https://github.com/Shin-Ogata/fieldwork/issues"
  },
  "devDependencies": {
    "@cdp/tasks": "file:packages/tool/tasks",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-multi-entry": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-replace": "3.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/jasmine": "3.10.3",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "autoprefixer": "10.4.2",
    "clean-css": "5.2.2",
    "commander": "8.3.0",
    "convert-source-map": "1.8.0",
    "cross-conf-env": "1.2.1",
    "del": "6.0.0",
    "dts-bundle-generator": "6.4.0",
    "es6-plato": "git+https://github.com/the-simian/es6-plato.git",
    "eslint": "8.7.0",
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "html-minifier": "4.0.0",
    "http-server": "14.1.0",
    "https-proxy-agent": "5.0.0",
    "jasmine": "4.0.2",
    "jasmine-core": "4.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "postcss": "8.4.5",
    "postcss-cli": "9.1.0",
    "requirejs": "2.3.6",
    "rollup": "2.64.0",
    "sass": "1.48.0",
    "semver": "7.3.5",
    "tar": "6.1.11",
    "terser": "5.10.0",
    "testem": "3.6.0",
    "typedoc": "0.22.10",
    "typescript": "4.5.4"
  },
  "homepage": "https://github.com/Shin-Ogata/fieldwork#readme",
  "keywords": [
    "C++",
    "TypeScript",
    "feasibility",
    "javascript"
  ],
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shin-Ogata/fieldwork.git"
  },
  "scripts": {
    "check-update": "cdp-task delegate check-update --exists && npm outdated || exit 0",
    "ci": "run-s packages:ci metrics",
    "clean:doc": "cdp-task clean -d",
    "clean:doc:a": "cdp-task clean -t ./docs/typedoc",
    "clean:doc:c": "cdp-task clean -t ./docs/reports/coverage",
    "clean:doc:m": "cdp-task clean -t ./docs/reports/metrics",
    "install": "cdp-task delegate install --preset -- --no-save --no-audit --no-bin-links --no-optional --no-update-notifier --no-package-lock --legacy-peer-deps --loglevel=error --progress=false",
    "lint": "cdp-task delegate lint",
    "metrics": "run-p metrics:*",
    "metrics:file": "cdp-task metrics -r file -l ./config/lint/eslint.js -p lib",
    "metrics:module": "cdp-task metrics -r module -l ./config/lint/eslint.js -p lib,framework,extension",
    "packages:ci": "cdp-task delegate ci -l lib,framework,extension",
    "packages:clean": "cdp-task delegate clean -l lib,framework,extension",
    "packages:clear": "cdp-task delegate clear",
    "set-version": "cross-conf-env cdp-task delegate set-version -l tool,lib,framework,extension -- npm_package_version",
    "set-version:to": "cdp-task delegate set-version -l tool,lib,framework,extension --",
    "test": "npm run ci"
  }
}
