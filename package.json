{
  "name": "fieldwork",
  "description": "feasibility study",
  "version": "0.9.7",
  "author": "Shin Ogata",
  "bugs": {
    "url": "https://github.com/Shin-Ogata/fieldwork/issues"
  },
  "devDependencies": {
    "@cdp/tasks": "file:./packages/tool/tasks",
    "@rollup/plugin-alias": "3.1.2",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-multi-entry": "4.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "@types/fs-extra": "9.0.11",
    "@types/jasmine": "3.7.7",
    "@types/node": "15.14.0",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "autoprefixer": "10.2.6",
    "chalk": "4.1.1",
    "clean-css": "5.1.3",
    "commander": "8.0.0",
    "convert-source-map": "1.8.0",
    "cross-conf-env": "1.2.1",
    "del": "6.0.0",
    "dts-bundle-generator": "5.9.0",
    "es6-plato": "1.2.3",
    "eslint": "7.29.0",
    "fs-extra": "10.0.0",
    "glob": "7.1.7",
    "html-minifier": "4.0.0",
    "http-server": "0.12.3",
    "https-proxy-agent": "5.0.0",
    "jasmine": "3.8.0",
    "node-gyp": "8.1.0",
    "node-sass": "6.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "postcss": "8.3.5",
    "postcss-cli": "8.3.1",
    "rollup": "2.52.6",
    "semver": "7.3.5",
    "tar": "6.1.0",
    "terser": "5.7.1",
    "testem": "3.4.2",
    "typedoc": "0.21.2",
    "typescript": "4.3.5"
  },
  "homepage": "https://github.com/Shin-Ogata/fieldwork#readme",
  "keywords": [
    "C++",
    "TypeScript",
    "feasibility",
    "javascript"
  ],
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shin-Ogata/fieldwork.git"
  },
  "scripts": {
    "check-update": "cdp-task delegate check-update && npm outdated || exit 0",
    "ci": "run-s packages:ci metrics",
    "clean:doc": "cdp-task clean -d",
    "clean:doc:a": "cdp-task clean -t ./docs/typedoc",
    "clean:doc:c": "cdp-task clean -t ./docs/reports/coverage",
    "clean:doc:m": "cdp-task clean -t ./docs/reports/metrics",
    "install": "cdp-task delegate install --preset -- --no-audit --no-bin-links --no-optional --no-update-notifier --loglevel=error --progress=false",
    "lint": "cdp-task delegate lint",
    "metrics": "run-p metrics:*",
    "metrics:file": "cdp-task metrics -r file -l ./config/lint/eslint.js -p lib",
    "metrics:module": "cdp-task metrics -r module -l ./config/lint/eslint.js -p lib,framework,extension",
    "packages:ci": "cdp-task delegate ci -l lib,framework,extension",
    "packages:clean": "cdp-task delegate clean -l lib,framework,extension",
    "packages:clear": "cdp-task delegate clear",
    "set-version": "cross-conf-env cdp-task delegate set-version -l tool,lib,framework,extension -- npm_package_version",
    "set-version:to": "cdp-task delegate set-version -l tool,lib,framework,extension --",
    "test": "npm run ci"
  }
}
