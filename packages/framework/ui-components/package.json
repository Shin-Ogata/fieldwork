{
  "name": "@cdp/ui-components",
  "description": "ui-componets collection",
  "version": "0.9.17",
  "author": "Shin Ogata",
  "dependencies": {
    "@cdp/runtime": "file:../runtime"
  },
  "devDependencies": {
    "@cdp/ui-utils": "file:../../lib/ui/utils",
    "@cdp/ui-forms": "file:../../lib/ui/forms",
    "@cdp/ui-listview": "file:../../lib/ui/listview"
  },
  "keywords": [
    "CDP",
    "TypeScript",
    "javascript"
  ],
  "license": "UNLICENSED",
  "main": "dist/ui-components",
  "module": "dist/ui-components.mjs",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shin-Ogata/fieldwork.git"
  },
  "scripts": {
    "build": "run-s -l build:*",
    "build:src": "run-s compile:src bundle:src bundle:dts minify:src",
    "build:test": "run-s compile:test bundle:test",
    "bundle": "run-p bundle:*",
    "bundle:dts": "cdp-task bundle dts --config ./build.config.js",
    "bundle:src": "run-p bundle:src:*",
    "bundle:src:built": "rollup -c ./build.config.js",
    "bundle:test": "rollup -c ./tests/test.config.js",
    "check": "npm run set-version:scss",
    "ci": "run-s -l clean:all build:src build:test lint unit-test:ci typedoc",
    "clean": "cdp-task clean",
    "clean:all": "cdp-task clean -a",
    "clean:doc": "cdp-task clean -d",
    "clean:doc:a": "cdp-task clean -t ./docs/typedoc",
    "clean:doc:c": "cdp-task clean -t ./docs/reports/coverage",
    "clean:doc:m": "cdp-task clean -t ./docs/reports/metrics",
    "clear": "npm run clean && npm run clean:doc && cdp-task clean -t ./node_modules && cdp-task clean -t ./types/@cdp && cdp-task clean -t ./tests/unit/@cdp",
    "compile": "run-s compile:*",
    "compile:src": "run-p compile:src:*",
    "compile:src:scss": "run-s compile:src:scss:*",
    "compile:src:scss:compile": "sass --style=expanded --source-map --embed-source-map src/scss:.temp/src/scss",
    "compile:src:scss:prefix": "postcss .temp/src/scss/**/*.css --use autoprefixer -d dist",
    "compile:src:ts": "tsc -p ./tsconfig.json",
    "compile:test": "tsc -p ./tests/tsconfig.json",
    "install": "npm run setup",
    "lint": "eslint src -c ../../../config/lint/tslint.js --ext .ts",
    "metrics": "run-s -l metrics:*",
    "metrics:file": "cdp-task metrics -r file -l ../../../config/lint/eslint.js",
    "metrics:module": "cdp-task metrics -r module -l ../../../config/lint/eslint.js",
    "minify": "run-p minify:*",
    "minify:src": "run-p minify:src:*",
    "minify:src:css": "cdp-task minify css --config ./build.config.js",
    "minify:src:js": "cdp-task minify js --config ./build.config.js",
    "postset-version": "node ./tasks/set-version-stylesheet.js",
    "set-version": "cdp-task set-version",
    "setup": "cdp-task bundle setup-ut",
    "test": "run-s -l lint unit-test:ci",
    "typedoc": "npm run clean:doc:a && typedoc dist/ui-components.d.ts --options ../../../config/doc/typedoc.js --excludeExternals",
    "unit-test": "cdp-task ut -c ./tests/test.config.js",
    "unit-test:ci": "cdp-task ut ci -c ./tests/test.config.js -s",
    "ut": "npm run unit-test",
    "ut:ci": "npm run unit-test:ci"
  },
  "types": "dist/ui-components.d.ts"
}
